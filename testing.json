{
    "manifest_version": 3,
    "name": "Trash Folder",
    "description": "Base Level Extension",
    "version": "1.0",
    "content_scripts" : [
      {
        "matches" : [
          "<all_urls>"
        ],
        "js": ["content.js"]
      }
    ],
    "background" : {
      "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "playing_cards.png"
    }
  }